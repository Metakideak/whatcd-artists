{
  "name": "What.CD Artists",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Bulk add artists to your What.CD upload",
  "homepage_url": "https://github.com/Sovietaced/whatcd-artists",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "https://what.cd/upload.php"
  ]
}